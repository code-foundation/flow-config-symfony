{
    "name": "code-foundation/flow-config-symfony",
    "description": "Symfony bundle for code-foundation/flow-config",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Edward Murrell",
            "email": "edward.murrell@codefoundation.com.au"
        }
    ],
    "minimum-stability": "stable",
    "require": {
	"php": ">=7.1.0",
        "code-foundation/flow-config": "^0.0.2",
        "symfony/framework-bundle": "^4.0",
        "doctrine/doctrine-bundle": "^1.11",
        "symfony/dependency-injection": "^4.0",
        "symfony/yaml": "^4.3",
        "symfony/config": "^4.0",
        "symfony/http-kernel": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "CodeFoundation\\FlowConfigBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeFoundation\\FlowConfigBundle\\Tests\\": "tests/"
        }
    }
}
